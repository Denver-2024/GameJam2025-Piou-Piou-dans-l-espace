[sub_resource type="GDScript" id="GDScript_23w0p"]
script/source = "extends Panel

@onready var nb_manches : LineEdit = $MarginContainer/VBoxContainer/manches/LineEdit
@onready var temps_manches : LineEdit = $MarginContainer/VBoxContainer/temps_manches/LineEdit
@onready var temps_inter : LineEdit = $MarginContainer/VBoxContainer/Temp_inter_manches/LineEdit
@onready var events : CheckButton = $MarginContainer/VBoxContainer/events/CheckButton

@onready var button_back_pressed : Button =$MarginContainer/VBoxContainer/parameters2/Back
@onready var start_button_pressed : Button = $MarginContainer/VBoxContainer/parameters2/Start

@export var pnb_manches : int = 3
@export var tps_manches : int = 120
@export var tps_inter : int = 5
@export var events_enable :bool = true

@onready var HUD : player_HUD = $\"../../..\"
@onready var printer : chat_log = $\"../../..\".get_parent().get_parent().get_parent().get_parent().get_node(\"UI/MarginContainer/VBoxContainer/HBoxContainer2/Panel/RichTextLabel\")
@onready var G_manager : game_manager =  $\"../../..\".get_parent().get_parent().get_parent().get_parent().get_node(\"Timer\")
@onready var player : Base_Player = $\"../../..\".get_parent().get_parent().get_parent()

func _ready() -> void:
	button_back_pressed.connect(\"pressed\", Callable(self, \"back_pressed\"))
	start_button_pressed.connect(\"pressed\", Callable(self, \"start_pressed\"))

func back_pressed():
	if player.name.to_int() != multiplayer.get_unique_id(): return
	pnb_manches = int(nb_manches.text)
	tps_manches = int(temps_manches.text)
	tps_inter = int(temps_inter.text)
	events_enable = events.is_pressed()
	G_manager.upt_var.rpc(pnb_manches, tps_manches, tps_inter, events_enable)
	HUD.windows(false, \"party_parameters\")
	
func start_pressed():
	if player.name.to_int() != multiplayer.get_unique_id(): return
	HUD.windows(false, \"party_parameters\")
	pnb_manches = int(nb_manches.text)
	tps_manches = int(temps_manches.text)
	tps_inter = int(temps_inter.text)
	events_enable = events.is_pressed()
	G_manager.upt_var.rpc(pnb_manches, tps_manches, tps_inter, events_enable)
	await get_tree().create_timer(0.5).timeout
	G_manager.debut_jeu.rpc()
"

